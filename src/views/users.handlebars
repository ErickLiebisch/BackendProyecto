<h1>Users</h1>
<article>
<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Age</th>
        <th>Role</th>
        <th>Last Connection</th>
    </tr>
    <tr>
        {{#each users}}
            <tr>
                <td>{{this.first_Name}}</td>
                <td>{{this.last_Name}}</td>
                <td>{{this.email}}</td>
                <td>{{this.age}}</td>
                <td>{{this.role}}</td>
                <td>{{this.last_Connection}}</td>
            </tr>
        {{/each}}
    </tr>
</table>
</article>